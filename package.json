{
  "name": "faster-titanium",
  "version": "1.0.0",
  "description": "faster Titanium",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "spec"
  },
  "scripts": {
    "build": "babel src/{server|hook} -d dist",
    "app-dev": "nodemon -w src/titanium --exec 'npm run bundle && npm run build-app'",
    "build-app": "ti build -p ios --faster --project-dir test/faster-titanium-app",
    "bundle": "browserify src/titanium/faster-titanium.js -t babelify --outfile dist/app.js",
    "test": "mocha --compilers js:espower-babel/guess spec/**/*.spec.js",
    "server-dev": "DEBUG=faster-titanium:* nodemon -w src/server --exec babel-node -- src/server/main-process.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CureApp/faster-titanium.git"
  },
  "keywords": [
    "titanium",
    "appcelerator",
    "faster"
  ],
  "author": "CureApp, Inc.",
  "dependencies": {
    "chokidar": "^1.4.2",
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "esdoc": "^0.4.4",
    "esdoc-es7-plugin": "0.0.3",
    "espower-babel": "^4.0.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "power-assert": "^1.2.0",
    "titanium": "^5.0.6"
  }
}
